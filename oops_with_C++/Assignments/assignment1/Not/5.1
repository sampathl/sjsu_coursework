#include <iostream>
using namespace std;

int main()
{   
   cout << "m: " << endl;
   int m;
   cin >> m;
   cout << "n: " << endl;
   int n;
   cin >> n;
   int result = 0;
   int n1=0;
   int m1=0;
   int dn=0;
   int dm=0;
   int dn1=0;
   int dn2=0;
	while (n1!=0)
	{
		dn=dn+1;
		n1=n1/10;
	}
	
	while (m1!=0)
	{
		dm=dm+1;
		m1=m1/10;
	}
	n1=n;
	m1=m;
	dn1=dn;
	dn2=dn;
	int dm1=dm;
	int check=0;
	int counter=0;
	int rm1=0;
	
	while (dn1!=0)
	{
		r1=n1%10;
		dn2= dn1 -1;
		n2=n1/10;
		
		while (dn2!=0)
		{
			r2=n2%10;
			if (r1==r2)
			{
				check=1;
				break;
			}
			
			dn2=dn2-1;
			n2=n2=1;
		}
		if (check!=1)
		{
			while(dm1!=0)
			{
				rm1 = m1%10;
				if (rm1 == r1)
				{
					counter=counter+1;
					break;
				}
				dm1=dm1-1;
				m1=m1-1;
			}
		}
	dn1=dn1-1;
	n1=n1/10;
	}
	result= counter;	
   cout << "Count: " << result << endl;
   return 0;
}
